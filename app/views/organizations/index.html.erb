<div class="container">
  <h1>All Organizations</h1>
  <% @organizations.each do |org| %>
  </br>
  <div class="card">
  <h5 class="card-header"><%= link_to org.name, organization_path(org.flags) %></h5>
  <div class="card-body">
    <h5 class="card-title">Positive flag - please contact - <%= org.email %></h5>
  </div>
</div>
</br>
<% end %>
</div>
